    - sln
    EXTENSION tar.gz.sig
    - tar.gz.sig
    EXTENSION txt
    - txt
    EXTENSION webarchive    # Safari saved webpage
    - webarchive
    EXTENSION xps           # Microsoft paper specification file
    - xps

